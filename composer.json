{
    "name": "dcolsay/laravel-xml",
    "description": "Handle xml",
    "authors": [
        {
            "name": "Boris DEHOUMON",
            "email": "borisdehoumon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.6",
        "illuminate/contracts": "^8.0",
        "prewk/xml-string-streamer": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.15"
    },
    "autoload": {
        "psr-4": {
            "Dcolsay\\XML\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcolsay\\XML\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcolsay\\XML\\XMLServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
